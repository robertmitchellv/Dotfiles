# My r-profile settings

# tell R where to find my packages
.libPaths(c("~/Code/R-libs", .libPaths() ) )

# options
options(stringsAsFactors = FALSE)

options(editor = "vim")

options(prompt = "--> ")
options(continue = "···  ")

# enforce margin
# options(width = 80)

q <- function (save = "no", ...) {
  quit(save = save, ...)
}

